{
    "plugin-name": ".lip and .fuz file maker",
    "author": "DanRuta, ionite",
    "nexus-link": "https://www.nexusmods.com/skyrimspecialedition/mods/55605",
    "plugin-version": "1.3",
    "plugin-short-description": "A plugin to automatically create .lip and .fuz files from synthesized audio lines",
    "min-app-version": "2.1.1",
    "install-requires-restart": false,
    "uninstall-requires-restart": false,

    "front-end-style-files": [],

    "front-end-hooks": {
        "start": {
            "post": {
                "file": "main.js",
                "function": "setupSettings"
            }
        }
    },
    "back-end-hooks": {
        "output-audio": {
            "post": {
                "file": "main.py",
                "function": "create_lip_fuz_files"
            }
        },
        "mp-output-audio": {
            "post": {
                "file": "main.py",
                "function": "mp_create_lip_fuz_files"
            }
        }
    }
}